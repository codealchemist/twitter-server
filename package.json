  "homepage": "https://github.com/codealchemist/twitter-server#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "npm-check": "^5.2.3",
    "standard": "^8.0.0"
  },
  "dependencies": {
